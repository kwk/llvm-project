#!/bin/bash
#<white>vim:syntax=sh 

function git_commit_time_yyyymmdd {
    local commit_timestamp=$(git --no-pager log -1 --format="%ct")
    echo -n $(date +%Y%m%d --date=@${commit_timestamp})
}

function git_author_time_yyyymmdd {
    local commit_timestamp=$(git --no-pager log -1 --format="%at")
    echo -n $(date +%Y%m%d --date=@${commit_timestamp})
}
